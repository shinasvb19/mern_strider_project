                                                                <input type="text" class="form-control" placeholder="in days"
                                                                    name="expiry" aria-label="in days" required>
                                                    <!-- </div> -->
                                                    <div class="text-danger">
                                                      
                                                    </div>
                                                    <input type="submit"
                                                        class="btn-blue btn-primary btn-md rounded mt-3 " type="submit">
                                                    </input>

                                                </form>


                                                <div class="col-lg-12 grid-margin stretch-card">
