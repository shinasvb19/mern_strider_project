                                                                    <thead>
                                                                        <tr>
                                                                            <th>NAME</th>
                                                                            <th>DISCOUNT</th>
                                                                            <th>EXPIRY DATE</th>
                                                                            <th>DELETE</th>

                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                     
                                                                            <%couponsFind.forEach(coupons=>{ %> 
                                                                            <tr>
                                                                                <td>
                                                                                 <%=coupons.couponName %> 
                                                                                </td>
                                                                                <td>
                                                                                    <%=coupons.dicount%>%
                                                                                </td>
                                                                                <td>
                                                                                    <%let date = coupons.expiryDate.toLocaleDateString()  %>
                                                                                    <%= date%> 
                                                                                </td>
                                                                                <td>
                                                                                   
                                                                                        <button class="btn btn-danger" data-toggle="modal"
                                                                                        data-target="#exampleModalCenter" onclick="pop('<%=coupons._id%>')" >delete</button>
                                                                                          
                                                                                </td>
                                                                            </tr>
                                                                       <%  }) %>  
                                                                          
                                                                    </tbody>
                                                                </table>
                                                             
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

