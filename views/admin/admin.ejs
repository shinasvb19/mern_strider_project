        <script src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/3.0.279/pdf.min.js" integrity="sha512-QJy1NRNGKQoHmgJ7v+45V2uDbf2me+xFoN9XewaSKkGwlqEHyqLVaLtVm93FzxVCKnYEZLFTI4s6v0oD0FbAlw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.2/jspdf.debug.js"> </script>
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        <script>
            const packed =document.getElementById('packed').value
            const shipped =document.getElementById('shipped').value
            const cancelled=document.getElementById('cancelled').value
            const ordered =document.getElementById('ordered').value
            const delivered =document.getElementById('delivered').value
            console.log(delivered)
            const labels = [
                'packed',
                'shipped',
                'ordered',
                'delivered',
                'cancelled',
            ];
          
            const data = {
                labels: labels,
                datasets: [{
                    label: 'My First dataset',
                    backgroundColor: [
                        'rgb(255, 99, 132)',
                        'rgb(54, 162, 235)',
                        'rgb(255, 205, 86)',
                        'rgb(175,225,175)',
                        'rgb(175,225,80)'
                    ],
                    data: [ packed,
                shipped,
                ordered,
                delivered,
                cancelled]
                }]
            };
            const config = {
                type: 'doughnut',
                data: data,
                options: {}
            };
            const orderStatusDoughnut = new Chart(
                document.getElementById('orderStatusDoughnut'),
                config
            );
            function downloadPDF(){
                const canvas = document.getElementById('sales-chart')
                const canvasImage = canvas.toDataURL('image/jpeg', 1.0);
            // console.log(canvasImage);
            let pdf = new jsPDF('landscape')
            pdf.setFontSize(20)
            pdf.addImage(canvasImage,'JPEG',15,15,260,150)
           pdf.save('sales.pdf')
            }
        </script>
